<!-- Блок Featured Services -->
<div class="feature_section104">
    <div class="container-fluid">
        <div class="row">
            <h1 class="caps"><strong>Воплощение идеи начинается с простых шагов</strong></h1>
            <div class="col-sm-offset-3 col-md-offset-3 col-lg-offset-3 col-xs-12 col-sm-6 col-md-6 col-lg-6">
                
                <div class="cforms">
                    @using (Html.BeginForm("Contact", "PSe"))
                    {

                        <label class="label">Имя    <span class="err pull-right">@Html.ValidationMessageFor(m => m.Name)</span></label>
                        <label class="input">
                            @Html.TextBoxFor(m => m.Name, new { @class = "mmail" })
                        </label>

                        <div class="clearfix"></div>

                        <label class="label">E-mail    <span class="err pull-right">@Html.ValidationMessageFor(m => m.Email)</span></label>
                        <label class="input">
                            @Html.TextBoxFor(m => m.Email, new { @class = "mmail" })
                        </label>

                        <label class="label">Тема    <span class="err pull-right">@Html.ValidationMessageFor(m => m.Subject)</span></label>
                        <label class="input">
                            @Html.TextBoxFor(m => m.Subject, new { @class = "mmail" })
                        </label>

                        <label class="label">Текст сообщения   <span class="err pull-right">@Html.ValidationMessageFor(m => m.Message)</span></label>
                        <label class="textarea">
                            @Html.TextAreaFor(m => m.Message, new { rows = 5, @class = "mmail"})
                        </label>
                        <label class="label">
                            <button type="submit" class="btn four">Запустить процесс</button>
                        </label>
                    }
                </div>
                <div class="clearfix margin_bottom2"></div>
            </div>
        </div>

    </div>
</div>
<!-- end Блок Featured Services -->


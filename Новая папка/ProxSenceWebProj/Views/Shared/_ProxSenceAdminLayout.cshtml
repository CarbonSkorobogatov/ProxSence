<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="keywords" content="" />
    <meta name="description" content="" />


    <script src="~/Scripts/jquery-3.1.1.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>

    <link rel="stylesheet" type="text/css" href="~/Styles/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Styles/bootstrap-theme.min.css" />


    <!-- Фавикон -->
    <link rel="shortcut icon" href="~/Images/favicon.png">

    <!-- Google fonts -->
    <link href="https://fonts.googleapis.com/css?family=Jura:400,500,600|Open+Sans|Titillium+Web:300,400,600,900" rel="stylesheet" type='text/css'>

</head>
<body>
    <div>
        @if(TempData["message"] != null)
        {
            <div class="alert alert-success">
                @TempData["message"]
            </div>
        }
        @RenderBody()
    </div>
</body>



</html>
